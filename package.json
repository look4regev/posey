{
  "name": "posey",
  "version": "1.0.0",
  "description":
    "Game of mimicking poses and getting real time feedback using PoseNet and React.js",
  "engines": {
    "node": ">= 7.0.0"
  },
  "main": "index.js",
  "scripts": {
    "build-client": "webpack",
    "build-client-watch": "webpack -w",
    "deploy": "script/deploy",
    "heroku-token": "script/encrypt-heroku-auth-token",
    "lint": "eslint ./ --ignore-path .gitignore",
    "lint-fix": "npm run lint -- --fix",
    "precommit": "lint-staged",
    "preprocess": "nodemon --exec babel-node preprocess.js",
    "prepare": "if [ -d .git ]; then npm-merge-driver install; fi",
    "prettify": "prettier --write \"**/*.{js,jsx,json,css,scss,md}\"",
    "postinstall": "npm run build-client && touch secrets.js",
    "seed": "node script/seed.js",
    "start": "node server",
    "start-dev":
      "NODE_ENV='development' npm run build-client-watch & NODE_ENV='development' npm run start-server",
    "start-server":
      "nodemon server -e html,js,scss --ignore public --ignore client",
    "test":
      "NODE_ENV='test' mocha \"./server/**/*.spec.js\" \"./client/**/*.spec.js\" \"./script/**/*.spec.js\" --require @babel/polyfill --require @babel/register"
  },
  "lint-staged": {
    "*.{js,jsx}": ["prettier --write", "npm run lint-fix", "git add"],
    "*.{css,scss,json,md}": ["prettier --write", "git add"]
  },
  "author": "Regev Golan & Amir Keren",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/look4regev/posey.git"
  },
  "peerDependencies": {
    "@tensorflow/tfjs-core": "~1.2.1",
    "@tensorflow/tfjs-converter": "~1.2.1"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "canvas": "^2.6.0",
    "compression": "^1.7.3",
    "compute-cosine-similarity": "^1.0.0",
    "connect-session-sequelize": "^4.1.0",
    "dat.gui": "^0.7.5",
    "express": "^4.16.3",
    "express-session": "^1.15.1",
    "fs": "0.0.1-security",
    "history": "^4.6.3",
    "morgan": "^1.9.1",
    "passport": "^0.3.2",
    "passport-google-oauth": "^1.0.0",
    "pg": "^6.1.2",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.2",
    "react-router-dom": "^4.3.1",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.1",
    "redux-thunk": "^2.3.0",
    "sequelize": "^6.0.0",
    "socket.io": "^2.1.1",
    "stats.js": "^0.17.0",
    "style-loader": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-decorators": "7.0.0-beta.54",
    "@babel/plugin-proposal-export-namespace-from": "7.0.0-beta.54",
    "@babel/plugin-proposal-function-sent": "7.0.0-beta.54",
    "@babel/plugin-proposal-json-strings": "7.0.0-beta.54",
    "@babel/plugin-proposal-numeric-separator": "7.0.0-beta.54",
    "@babel/plugin-proposal-throw-expressions": "7.0.0-beta.54",
    "@babel/plugin-syntax-dynamic-import": "7.0.0-beta.54",
    "@babel/plugin-syntax-import-meta": "7.0.0-beta.54",
    "@babel/polyfill": "^7.0.0-beta.55",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0-beta.55",
    "@babel/register": "^7.0.0-beta.55",
    "@tensorflow-models/posenet": "~2.1.3",
    "@tensorflow/tfjs-converter": "~1.2.1",
    "@tensorflow/tfjs-core": "~1.2.1",
    "axios-mock-adapter": "^1.15.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.0.0-beta.4",
    "chai": "^3.5.0",
    "css-loader": "^3.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "eslint": "^4.19.1",
    "eslint-config-fullstack": "^5.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-react": "^7.10.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.3",
    "npm-merge-driver": "^2.3.5",
    "prettier": "1.11.1",
    "react-test-renderer": "^16.4.2",
    "redux-devtools-extension": "^2.13.5",
    "redux-mock-store": "^1.5.3",
    "supertest": "^3.1.0",
    "webpack": "^4.16.4",
    "webpack-cli": "^3.1.0"
  }
}
